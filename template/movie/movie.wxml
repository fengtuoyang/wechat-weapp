<import src='/template/stars/stars.wxml' />
<template name='movie'>
  <view class='movie-container' data-movieId='{{movieId}}' bindtap='openMovieDetail'>
    <image class='movie-img' src='{{images}}'></image>
    <text class='movie-name'>{{title}}</text>
    <template is='stars' data='{{...rating}}'></template>
  </view>
</template>